# Example production environment file
# Copy to /etc/cofound/env and fill in secure values (DO NOT commit secrets to git)

NODE_ENV=production
PORT=5000
DATABASE_URL=postgresql://username:password@db-host:5432/cofound_prod
REDIS_URL=redis://:password@redis-host:6379
JWT_SECRET=replace_with_secure_random_string
VITE_FRONTEND_URL=https://co-found.uz
VITE_API_URL=https://your-api-domain
LETSENCRYPT_EMAIL=admin@co-found.uz
SENTRY_DSN=
SENTRY_TRACES_SAMPLE_RATE=0.1
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Optional monitoring secrets
SLACK_WEBHOOK=
